{{#extends 'base'}}
{{#block 'content'}}
<h1>All Movies</h1>
<table class="table">
    
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>ISBN</th>
        </tr>
    </thead>
     {{#each books}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.isbn}}</td>
            </tr>
        {{/each}}
        </table>
{{/block}}
{{/extends}}